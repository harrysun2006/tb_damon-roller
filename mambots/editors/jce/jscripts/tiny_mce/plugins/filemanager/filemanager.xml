<?xml version="1.0" ?>
<mosinstall version="1.0.0" type="jceplugin" plugin="filemanager">
	<name>File Manager</name>
	<version>1.0.2</version>
	<creationDate>2005</creationDate>
	<author>Ryan Demmer</author>
	<authorEmail>ryandemmer@gmail.com</authorEmail>
	<authorUrl>http://www.cellardoor.a.net/jce/</authorUrl>
	<copyright>Ryan Demmer</copyright>
	<license>LGPL</license>
	<description><![CDATA[
    File Manager plugin for JCE, version 1.0.2<br />
    Changelog (from 1.0.1)<br />
    <ul>
        <li>Fixed permissions errors relating to missing isSuperAdmin variable.</li>
        <li>Added missing permission checks for Cut/Copy/Paste.</li>
    </ul>
    Changelog (from 1.0.0)<br />
    <ul>
        <li>Fixed corrupt pdf icon.</li>
        <li>Cleaned up authentication routines.</li>
        <li>Removed redundant authentication check.</li>
    </ul>
    ]]></description>
	<icon>filemanager</icon>
	<layout_icon>filemanager</layout_icon>
	<elements></elements>
	<files>
        <filename plugin="filemanager">editor_plugin.js</filename>
        <filename>index.html</filename>
        <filename>blank.htm</filename>
        <filename>manager.php</filename>
        <filename>files.php</filename>
        <filename>index.php</filename>
        <filename>classes/manager.class.php</filename>
        <filename>classes/index.html</filename>
        <filename>css/manager.css</filename>
        <filename>css/files.css</filename>
        <filename>css/index.html</filename>
        <filename>docs/index.html</filename>
        <filename>docs/help.php</filename>
        <filename>docs/images/index.html</filename>
        <filename>docs/images/en/index.html</filename>
        <filename>docs/images/en/fm_insert1.jpg</filename>
        <filename>docs/images/en/fm_insert2.jpg</filename>
        <filename>docs/images/en/fm_layout.jpg</filename>
        <filename>docs/images/en/upload.jpg</filename>
        <filename>images/filemanager.gif</filename>
        <filename>images/index.html</filename>
        <filename>images/ext/def_small.gif</filename>
        <filename>images/ext/doc_small.gif</filename>
        <filename>images/ext/swf_small.gif</filename>
        <filename>images/ext/gif_small.gif</filename>
        <filename>images/ext/gz_small.gif</filename>
        <filename>images/ext/htm_small.gif</filename>
        <filename>images/ext/html_small.gif</filename>
        <filename>images/ext/jpg_small.gif</filename>
        <filename>images/ext/mov_small.gif</filename>
        <filename>images/ext/pdf_small.gif</filename>
        <filename>images/ext/png_small.gif</filename>
        <filename>images/ext/ppt_small.gif</filename>
        <filename>images/ext/rar_small.gif</filename>
        <filename>images/ext/txt_small.gif</filename>
        <filename>images/ext/wmv_small.gif</filename>
        <filename>images/ext/xls_small.gif</filename>
        <filename>images/ext/zip_small.gif</filename>
        <filename>jscripts/manager.js</filename>
        <filename>jscripts/files.js</filename>
        <filename>jscripts/index.html</filename>
        <filename>langs/en.js</filename>
        <filename>langs/en.php</filename>
        <filename>langs/index.html</filename>
    </files>
	<params>
        <param name="dir" type="text" default="/images/stories" label="File Directory Path" description="Path to file directory."/>
        <param name="url" type="text" default="/images/stories" label="File Directory URL" description="URL to file directory."/>
        <param name="ext" type="text" size="80" default="html,doc,xls,txt,gif,jpeg,jpg,png,pdf,zip,swf,rar,tar,gz,mov,wmv,wav" label="Allowed Extensions" description="Comma seperated list of allowed extensions."/>
        <param name="view" type="text" size="80" default="html,doc,xls,txt,gif,jpeg,jpg,png,pdf,swf,mov,wmv,wav" label="Viewable Files" description="Comma seperated list of viewable files."/>
        <param name="size" type="text" default="2" label="Maximum File size(MB)" description="Maximum file size for uploading (MB)."/>
		<param name="@spacer" type="spacer" default="" label="File Manager Access" description="" />
        <param name="upload" type="list" default="18" label="Allow Upload" description="Select access level to allow Uploads">
            <option value="0">**Disabled**</option>
			<option value="18">Registered</option>
			<option value="19">-Author</option>
			<option value="20">--Editor</option>
			<option value="21">---Publisher</option>
			<option value="23">----Manager</option>
			<option value="24">-----Administrator</option>
			<option value="25">------Super Administrator</option>
		</param>
		<param name="folder_new" type="list" default="18" label="Allow Folder Create" description="Select access level to allow Folder Create">
            <option value="0">**Disabled**</option>
			<option value="18">Registered</option>
			<option value="19">-Author</option>
			<option value="20">--Editor</option>
			<option value="21">---Publisher</option>
			<option value="23">----Manager</option>
			<option value="24">-----Administrator</option>
			<option value="25">------Super Administrator</option>
		</param>
		<param name="folder_del" type="list" default="18" label="Allow Folder Delete" description="Select access level to allow Folder Delete">
            <option value="0">**Disabled**</option>
			<option value="18">Registered</option>
			<option value="19">-Author</option>
			<option value="20">--Editor</option>
			<option value="21">---Publisher</option>
			<option value="23">----Manager</option>
			<option value="24">-----Administrator</option>
			<option value="25">------Super Administrator</option>
		</param>
		<param name="folder_ren" type="list" default="18" label="Allow Folder Rename" description="Select access level to allow Folder Renaming">
            <option value="0">**Disabled**</option>
			<option value="18">Registered</option>
			<option value="19">-Author</option>
			<option value="20">--Editor</option>
			<option value="21">---Publisher</option>
			<option value="23">----Manager</option>
			<option value="24">-----Administrator</option>
			<option value="25">------Super Administrator</option>
		</param>
		<param name="file_del" type="list" default="18" label="Allow File Deleteing" description="Select access level to allow File Deleting">
            <option value="0">**Disabled**</option>
			<option value="18">Registered</option>
			<option value="19">-Author</option>
			<option value="20">--Editor</option>
			<option value="21">---Publisher</option>
			<option value="23">----Manager</option>
			<option value="24">-----Administrator</option>
			<option value="25">------Super Administrator</option>
		</param>
		<param name="file_ren" type="list" default="18" label="Allow File Renaming" description="Select access level to allow File Renaming">
            <option value="0">**Disabled**</option>
			<option value="18">Registered</option>
			<option value="19">-Author</option>
			<option value="20">--Editor</option>
			<option value="21">---Publisher</option>
			<option value="23">----Manager</option>
			<option value="24">-----Administrator</option>
			<option value="25">------Super Administrator</option>
		</param>
		<param name="file_move" type="list" default="18" label="Allow File Cut/Copy/Paste" description="Select access level to allow File Cut/Copy/Paste">
            <option value="0">**Disabled**</option>
			<option value="18">Registered</option>
			<option value="19">-Author</option>
			<option value="20">--Editor</option>
			<option value="21">---Publisher</option>
			<option value="23">----Manager</option>
			<option value="24">-----Administrator</option>
			<option value="25">------Super Administrator</option>
		</param>
		<param name="user_dir" type="list" default="0" label="Use Usertype/Username Access">
            <option value="0">No</option>
            <option value="1">Yes</option>
        </param>
        <param name="dir_type" type="list" default="level" label="Restriction Type">
            <option value="level">Usertype</option>
            <option value="name">Username</option>
            <option value="level_name">Usertype/Username</option>
        </param>
        <param name="user_dir_level" type="list" default="18" label="Restriction Level">
			<option value="18">Registered</option>
			<option value="19">-Author</option>
			<option value="20">--Editor</option>
			<option value="21">---Publisher</option>
			<option value="23">----Manager</option>
			<option value="24">-----Administrator</option>
			<option value="25">------Super Administrator</option>
		</param>
    </params>
</mosinstall>
