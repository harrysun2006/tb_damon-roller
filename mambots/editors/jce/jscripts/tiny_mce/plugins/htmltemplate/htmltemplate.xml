		<filename plugin="htmltemplate">editor_plugin.js</filename>
		<filename>blank.htm</filename>
		<filename>htmltemplate.php</filename>
		<filename>savehtmltemplate.php</filename>
		<filename>jscripts/htmltemplate.js</filename>
		<filename>jscripts/savehtmltemplate.js</filename>
        <filename>css/htmltemplate.css</filename>
		<filename>css/blank.css</filename>
		<filename>css/index.html</filename>
		<filename>images/htmltemplate.gif</filename>
		<filename>images/savehtmltemplate.gif</filename>
		<filename>images/templates.gif</filename>
		<filename>langs/en.js</filename>
		<filename>langs/index.html</filename>
	</files>
</mosinstall>
