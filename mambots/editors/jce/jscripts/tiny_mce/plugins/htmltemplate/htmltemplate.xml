<?xml version="1.0" ?>
<mosinstall version="1.0.0" type="jceplugin" plugin="htmltemplate">
	<name>HTML Template</name>
	<version>1.0.0</version>
	<creationDate>2005</creationDate>
	<author>Ryan Demmer</author>
	<authorEmail>N/A</authorEmail>
	<authorUrl>N/A</authorUrl>
	<copyright>Ryan Demmer</copyright>
	<license>LGPL</license>
	<description>HTML Template plugins for JCE</description>
	<icon>htmltemplate,savehtmltemplate</icon>
	<layout_icon>templates</layout_icon>
	<elements></elements>
	<params>
        <param name="@spacer" type="spacer" default="" label="HTML Template Options" description="" />
